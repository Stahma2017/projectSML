<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GoogleMapsBottomSheetBehavior_Layout">
        <attr name="behavior_peekHeight"/>
        <attr name="behavior_hideable" format="boolean" />
        <attr name="behavior_skipCollapsed" format="boolean" />
        <attr name="behavior_anchorHeight" format="dimension">
            <enum name="auto" value="-1"/>
        </attr>
        <attr name="behavior_anchorColor" format="color" />
        <attr name="behavior_collapsedColor" format="color" />
        <attr name="behavior_anchorTextColor" format="color" />
        <attr name="behavior_collapsedTextColor" format="color" />
        <attr name="behavior_header_layout" format="reference" />
        <attr name="behavior_content_layout" format="reference" />
        <attr name="behavior_parallax_layout" format="reference" />


    </declare-styleable>


    <declare-styleable name="AvatarImageBehavior">
        <attr name="finalYPosition" format="dimension"/>
        <attr name="startXPosition" format="dimension"/>
        <attr name="startToolbarPosition" format="dimension"/>
        <attr name="startHeight" format="dimension"/>
        <attr name="finalHeight" format="dimension"/>
    </declare-styleable>
</resources>